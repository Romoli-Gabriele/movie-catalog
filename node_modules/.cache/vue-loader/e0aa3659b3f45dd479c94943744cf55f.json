zzo = false\n            }\n\n            if (Math.round(x) > 5) {\n\n                return 5\n            }\n\n            return Math.round(x) > 0 ? Math.round(x) : 0\n        }\n            \n        },\n        \n        calcW(x) {\n            let y = this.calcF(x);\n\n            y = 5 - y;\n            if (this.mezzo && y > 0) {\n                y--;\n\n            }\n            \n            return y;\n        },\n\n    }\n\n}\n</script>\n\n<style scoped>\n\n</style>\n"]}]}